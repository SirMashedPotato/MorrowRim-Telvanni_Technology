<?xml version="1.0" encoding="utf-8" ?>
<Defs>
 
	<HediffDef>
		<defName>MorrowRim_HiddenWeatherChanged</defName>
		<label>hidden weather changed</label>
		<description>Used for Achievements, you should not see this.</description>
		<hediffClass>HediffWithComps</hediffClass>
		<defaultLabelColor>(0.8, 0.6, 0.4)</defaultLabelColor>
		<maxSeverity>1</maxSeverity>
		<comps>
		  <li Class="HediffCompProperties_Immunizable">
			<severityPerDayNotImmune>-100</severityPerDayNotImmune>
		  </li>
		</comps>
		<stages>
			<li>
				<label>changed</label>
				<becomeVisible>false</becomeVisible>
			</li>
		</stages>
	</HediffDef>
	
</Defs>